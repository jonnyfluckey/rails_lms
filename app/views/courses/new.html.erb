<h1>New Course</h1>

<%= form_with(model: @course, local: true, class: 'ui form') do |f| %>
  <div class="field">
    <%= f.label :course_name %>
    <%= f.text_field :course_name, required: true, placeholder: 'Course Name' %>
  </div>
  <%= f.submit nil, class: 'ui blue button inversted' %>
<% end %>